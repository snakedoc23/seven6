<!DOCTYPE html>
<html lang="pl">
<head>
  <title>Seven6</title>
	<meta charset="UTF-8">
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<%= render 'layouts/header' %>
	<div id="container">
		<% flash.each do |key, value|%>
		<div class="flash <%= key %>">
			<%= value %>
		</div>
		<% end %>
		<%= yield %>
		<div class="clear"></div>
		<%= debug(params) if Rails.env.development? %>
	</div>
</body>
</html>
